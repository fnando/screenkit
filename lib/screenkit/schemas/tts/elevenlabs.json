{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "$id": "https://github.com/fnando/screenkit/schemas/tts/elevenlabs.json",
  "title": "The ElevenLabs voice engine options",
  "type": "object",
  "required": ["voice_id"],
  "properties": {
    "voice_id": { "type": "string" },
    "output_format": {
      "type": "string",
      "enum": [
        "mp3_22050_32",
        "mp3_24000_48",
        "mp3_44100_32",
        "mp3_44100_64",
        "mp3_44100_96",
        "mp3_44100_128",
        "mp3_44100_192",
        "pcm_8000",
        "pcm_16000",
        "pcm_22050",
        "pcm_24000",
        "pcm_32000",
        "pcm_44100",
        "pcm_48000",
        "ulaw_8000",
        "alaw_8000",
        "opus_48000_32",
        "opus_48000_64",
        "opus_48000_96",
        "opus_48000_128",
        "opus_48000_192"
      ]
    },
    "model_id": { "type": "string" },
    "language_code": {
      "type": "string",
      "minLength": 2,
      "maxLength": 2,
      "pattern": "\\S+"
    },
    "previous_text": { "type": "string" },
    "next_text": { "type": "string" },
    "previous_request_ids": { "type": "array", "items": { "type": "string" } },
    "next_request_ids": { "type": "array", "items": { "type": "string" } },
    "voice_settings": {
      "type": "object",
      "properties": {
        "stability": {
          "type": "number",
          "minimum": 0,
          "maximum": 1
        },
        "similarity_boost": {
          "type": "number",
          "minimum": 0,
          "maximum": 1
        }
      },
      "style": {
        "type": "integer",
        "minimum": 0
      },
      "speed": { "type": "number" }
    }
  }
}
